#include "SPI/LabSpi.hpp"
#include "tasks.hpp"
#include "stdio.h"
#include "FreeRTOS.h"

void vReadTask(void *pvParamters){
    LabSpi spi = LabSpi();
    spi.initialize(16, LabSpi::tSPI, 12);
    spi.transfer(0x9F);
}

int main(){
    scheduler_add_task(new terminalTask(PRIORITY_HIGH));
    xTaskCreate(vReadTask, "Read1", 1024, NULL, PRIORITY_LOW, NULL);
    scheduler_start();
    return -1;
}



